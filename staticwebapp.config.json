{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/login",
            "redirect": "/.auth/login/aad"
        },
        {
            "route": "/logout",
            "redirect": "/.auth/logout"
          },
      {
        "route": "/*",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/api/*",
        "allowedRoles": ["authenticated"]
      }
    ],
    "mimeTypes": {
      ".json": "text/json"
    },
    "platform": {
        "apiRuntime": "dotnet-isolated:8.0"
    }
  }